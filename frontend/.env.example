# Robotics Model Optimization Platform - Frontend Configuration
# Copy this file to .env and update the values

# ============================================================================
# BACKEND API CONFIGURATION (REQUIRED)
# ============================================================================

# Backend API URL
# Development: http://localhost:8000
# Production: https://api.yourplatform.com
REACT_APP_API_URL=http://localhost:8000

# WebSocket server URL (usually same as API URL)
# Development: http://localhost:8000
# Production: https://api.yourplatform.com
REACT_APP_WS_URL=http://localhost:8000

# ============================================================================
# OPTIONAL CONFIGURATION
# ============================================================================

# Enable debug mode (default: false)
# Shows additional console logs and debug information
REACT_APP_DEBUG=false

# API request timeout in milliseconds (default: 30000)
# Increase for slow networks or large file uploads
REACT_APP_API_TIMEOUT=30000

# Enable service worker for offline support (default: false)
# Only enable in production
REACT_APP_ENABLE_SERVICE_WORKER=false
